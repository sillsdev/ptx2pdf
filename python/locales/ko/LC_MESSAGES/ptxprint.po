msgstr "책 끝과 책 끝 장식 사이의 간격"

#: lib/ptxprint/texpert.py:199
msgid "Mark Trigger Points"
msgstr "트리거 포인트 표시"

#: lib/ptxprint/texpert.py:200
msgid "Display trigger points in output"
msgstr "출력에 트리거 포인트 표시"

#: lib/ptxprint/texpert.py:201
msgid "Thumbtab rotated adjustment"
msgstr "엄지 탭 회전 조정"

#: lib/ptxprint/texpert.py:202
msgid "Shift thumbtab text"
msgstr "엄지손가락 탭 텍스트 이동"

