#!/usr/bin/make -f
export DH_VERBOSE = 1

export DEB_PYTHON_INSTALL_LAYOUT=deb
export SETUPTOOLS_USE_DISTUTILS=stdlib

export PYBUILD_NAME=ptxprint

export PYBUILD_SYSTEM=pyproject

export PYBUILD_DISABLE_python3.6=test
export PYBUILD_DISABLE_python3.7=test
export PYBUILD_DISABLE_python3.8=test
export PYBUILD_DISABLE_python3.9=test
export PYBUILD_DISABLE_python3.10=test

%:
	dh $@ --with python3 --buildsystem=pybuild -v 


